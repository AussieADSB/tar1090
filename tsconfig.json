{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "ES6",
    "module": "none",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowUmdGlobalAccess": true,
    "lib": [
      "ES2016",
      "DOM",
      "DOM.Iterable"
    ],
    "typeRoots": ["node_modules/@types", "./src/@types", "node_modules/ol/**"],
    "rootDir": "./src",
    "outDir": "./html"
  }
}
